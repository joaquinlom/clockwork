<h1>Alta de Puesto</h1>

<%= form_tag save_job_path(@membership)  do  %>
  <% if @schedule.errors.any? %>
    <p class = "alert alert-danger">Error al Guardar</p>
  <% end %>
  <div class="field">
    <%= label_tag :job, "Nombre" %>
    <%= text_field_tag :job %>
  </div>


  <div class="actions">
    <%= submit_tag "Guardar", :class =>"btn btn-success" %>
  </div>
    
<% end %>
