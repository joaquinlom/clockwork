<%= form_tag add_schedule_path(@schedule_det.schedule_id),method: "post" do  %>
  <% if @schedule_det.errors.any? %>
    <p class = "alert alert-danger">Error al Guardar</p>
  <% end %>
  <div class="field">
    <%= label_tag :entry_time, "Hora Entrada" %>
    <%= time_field_tag :entry_time, class: "form-control" %>
    </div>

    <div class = "field">
      <%= label_tag :exit_time, "Hora de Salida" %><br>
      <%= time_field_tag :exit_time , class: "form-control"%>
      </div>

      <div class = "field">
        <%= label_tag :week_day, "Dia de la Semana" %><br>
        <%= text_field_tag :week_day %>
      </div>
      <div class = "field">
        <%= label_tag :length, "Distancia" %><br>
        <%= text_field_tag :length%>
      </div>
      <fieldset class="gllpLatlonPicker">
        <div class = "field">
          <%= label_tag :latitude, "Latitud" %><br>
          <%= text_field_tag 'latitude',:latitude, class: 'gllpLatitude',:value=>28 %>
          </div>

          <div class = "field">
            <%= label_tag :longitude, "Longitud" %><br>
            <%= text_field_tag  'longitude',:longitude, class: 'gllpLongitude',:value=>-106 %>
            </div>

            <input type="text" class="gllpSearchField">
            <input type="button" class="gllpSearchButton" value="search">
            <div class="gllpMap" style='width: 800px; height: 400px;' >Google Maps</div>
          </fieldset>

          <div class="actions">
            <%= submit_tag "Guardar", :class =>"btn btn-success" %>
          </div>

        <% end %>

        <script src="http://maps.googleapis.com/maps/api/js?sensor=false&latitude=50&longitude=-80"></script>
